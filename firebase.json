{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls" : true,

    "rewrites": [{
      "source": "/Beavers",
      "destination": "/beavers.html"
    } , {
      "source": "/Cubs",
      "destination": "/cubs.html"
    } , {
      "source": "/Scouts",
      "destination": "/scouts.html"
    } , {
      "source": "/Senior Scouts",
      "destination": "/senior-scouts.html"
    } , {
      "source": "/senior scouts",
      "destination": "/senior-scouts.html"
    } , {
      "source": "/Rovers",
      "destination": "/rovers.html"
    } , {
      "source": "/login",
      "destination": "/login-page.html"
    } , {
      "source": "/Login",
      "destination": "/login-page.html"
    } , {
      "source": "/register",
      "destination": "/register-page.html"
    } , {
      "source": "/Register",
      "destination": "/register-page.html"
    } , {
      "source": "/Files",
      "destination": "/files.html"
    }]
  }
}

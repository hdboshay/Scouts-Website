{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls" : true,

    "rewrites": [ {
      "source": "/beavers",
      "destination": "/Beavers.html"
    } , {
      "source": "/cubs",
      "destination": "/Cubs.html"
    } , {
      "source": "/scouts",
      "destination": "/Scouts.html"
    } , {
      "source": "/senior scouts",
      "destination": "/Senior Scouts.html"
    } , {
      "source": "/rovers",
      "destination": "/Rovers.html"
    }]
  }
}
